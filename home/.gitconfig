[user]
  email = cmumford@chromium.org
  name = Chris Mumford
[color]
  diff = auto
  status = auto
  branch = auto
  ui = auto
[merge]
  tool = mymeld
  renameLimit = 9999999
[diff]
  external = $HOME/bin/gitdiff.py
  renames = true
  renameLimit = 999999
[alias]
  dd = diff --no-ext-diff
  l = log --graph --decorate --pretty=oneline --abbrev-commit
  ll = log --graph --decorate --pretty=medium --abbrev-commit
  mb = map-branches
  mfo = dd --no-ext-diff origin/main --name-only
  sno = show --name-only
  summary = log --max-count=1 --format='%s'
  vd = difftool --tool=vimdiff
[branch]
  autosetuprebase = always
[push]
  default = tracking
[core]
  autocrlf = true
  filemode = false
  deltaBaseCacheLimit = 1G
[rerere]
  enabled = 1
[mergetool "mymeld"]
  cmd = $HOME/bin/gitmerge $BASE $LOCAL $REMOTE $MERGED
[http]
  cookiefile = /usr/local/google/home/cmumford/.gitcookies
