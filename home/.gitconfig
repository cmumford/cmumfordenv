[user]
	email = cmumford@cmumford.com
	name = Chris Mumford
[color]
	diff = auto
	status = auto
	branch = auto
	ui = auto
[merge]
	tool = mymeld
	renameLimit = 9999999
[diff]
	external = $HOME/bin/gitdiff.py
	renames = true
	renameLimit = 999999
[alias]
	co = checkout
[branch]
	autosetuprebase = always
[push]
	default = tracking
[core]
	autocrlf = false
	filemode = false
	deltaBaseCacheLimit = 1G
	pager = less -r
[rerere]
	enabled = 1
[mergetool "mymeld"]
	cmd = $HOME/bin/gitmerge $BASE $LOCAL $REMOTE $MERGED
