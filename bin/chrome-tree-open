#!/usr/bin/env python

import go
import os
import sys

cmd_subfolder = os.path.join(go.Go().getval('t'))
if cmd_subfolder not in sys.path:
  sys.path.insert(0, cmd_subfolder)
from chromite.cbuildbot import tree_status

if tree_status.IsTreeOpen(throttled_ok=True):
  sys.exit(0)
else:
  print >> sys.stderr, "Chromium tree is closed"
  sys.exit(1)
